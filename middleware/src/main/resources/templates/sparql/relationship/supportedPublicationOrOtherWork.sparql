PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX vivo: <http://vivoweb.org/ontology/core#>

CONSTRUCT {
    ?supportedInformationResource rdfs:label ?label .
} WHERE {
    <[(${uri})]> vivo:supportedInformationResource ?supportedInformationResource .
    ?supportedInformationResource rdfs:label ?label .
}